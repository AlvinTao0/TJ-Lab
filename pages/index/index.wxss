/* pages/movieList/movieList.wxss */
.contain{
  padding-bottom: 100rpx;
}
.movie-item{
  padding:20rpx 30rpx;
  border-bottom: 1rpx solid #eee;
  overflow: hidden;
  height: 200rpx;
}
.movie-left{
  float: left;
  width: 20%;
  height: 100%;
}
.movie-right{
  float: left;
  width: 80%;
  height: 100%;
  box-sizing: border-box;
  padding: 10rpx;
}
.movie-left image{
  height: 100%;
  width: 100%;
}
.title-wrap{
  font-size: 40rpx;
  display: flex;
  justify-content: space-between;
}
.tag-wrap{
  font-size: 30rpx;
  margin-top: 50rpx;
}
.tag text{
  margin-right: 10rpx;
}
.actor-wrap{
  font-size: 30rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.score{
  color: #ff7700;
}
.year{
  font-size: 35rpx;
}
.gray{
  color: #777;
}
.fixed{
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
}
.tab-b{
  background: #FAFAFA;
  padding:3rpx;
  display: flex;
  justify-content: space-around;
  border-top: 1rpx solid #ccc;
}
.tab {
  text-align: center;
}
.tab-icon {
  display: inline-block;
  vertical-align: middle;
  margin-bottom: -4rpx;
  width: 40rpx;
  height: 50rpx;
}
.tab-icon-movie {
   background: url(data:image/png;base64,) center center / 100% auto no-repeat; 
}
.tab-icon-chat {
  background: url(data:image/png;base64,) center center / 100% auto no-repeat;
}
.tab-label{
  font-size: 26rpx;
  color: #7F8389;
  position: relative;
  text-align: center;
}

.tab.active .tab-label{
  color: #FF83FA;
}
.tab.active .tab-icon-movie {
  background: url(data:image/png;base64,) center center / 100% auto no-repeat;
}
.tab.active .tab-icon-chat {
  background: url(data:image/png;base64,) center center / 100% auto no-repeat;
}

.scrollWrap{
  position: fixed;
  top: 0;
  bottom: 210rpx;
  left: 0;
  right: 0;
}
.scrollBox{
  padding-top: 30rpx;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
::-webkit-scrollbar{width:0;height: 0;color:transparent;}
.scrollBox .item{
  padding: 0 30rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.scrollBox .item>view{
  float: left
}
.scrollBox .item.right>view{
  float: right
}
.scrollBox .item .headImg{
  margin-right: 15rpx;
  margin-left: 0;
}
.scrollBox .item.right .headImg{
  margin-right: 0;
  margin-left: 12rpx;
}
.scrollBox .item .headImg image{
  width: 80rpx;
  height: 80rpx;
  margin-top: 3rpx;
}
.scrollBox .item .txt{
  max-width:490rpx;
  font-size:30rpx;
  padding:20rpx 15rpx;
  box-sizing:border-box;
  background:#fff;
  border:1rpx solid #ccc;
  border-radius:20rpx;
}
.scrollBox .item.right .txt{
  background: #b0e46e;
}
.inputWrap{
  position: fixed;
  bottom: 102rpx;
  height: 80rpx;
  left: 0;
  right: 0;
  background: #eee;
  padding: 10rpx 20rpx;
}
.inputWrap .section {
  border-radius: 20rpx;
  background: #fff;
}
.inputWrap textarea {
  width: 650rpx;
  height:80rpx;
  font-size: 30rpx;
  padding: 0 15rpx;
}
.chatOpenBtn {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}